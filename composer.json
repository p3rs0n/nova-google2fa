{
    "name": "lifeonscreen/nova-google2fa",
    "description": "This package provides Google2FA to Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/JC5/recovery"
      }
    ],
    "require": {
        "php": ">=7.1.0",
        "bacon/bacon-qr-code": "^2.0",
        "jc5/recovery": "*",
        "pragmarx/google2fa-laravel": "v1.4.1"
    },
    "autoload": {
        "psr-4": {
            "Lifeonscreen\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lifeonscreen\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
